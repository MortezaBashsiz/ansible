{
  "log": {
    "loglevel": "debug"
  },
  "inbounds": [{
    "listen": "{{ instance.listen }}",
    "port": {{ instance.port }},
    "protocol": "{{ instance.protocol }}",
    "streamSettings": {
{% if instance.wsurl != "null" %}
			"network": "ws",
        "wsSettings": {
        "path": "/{{ instance.wsurl }}"
        }
{% endif %}
		},
    "settings": {
      "clients": [
{% for client in instance.clients %}
        {
          "id": "{{ lookup('password', '/dev/null chars=ascii_letters,digits') | to_uuid }}",
          "level": {{ client.level }},
          "alterId": {{ client.alterId }}
        }
{% if not loop.last %}
    ,
{% endif %}
{% endfor %}
      ]
    }
  }],
  "outbounds": [{
    "protocol": "freedom",
    "settings": {}
  },{
    "protocol": "blackhole",
    "settings": {},
    "tag": "blocked"
  }]
}

